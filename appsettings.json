{
    "ConnectionStrings": {
        "Postgres": "Host=localhost;Database=postgres;Username=postgres;Password=postgres",
        "Redis": "localhost"
    },
    "Serilog": {
        "Using": [
            "Serilog.Sinks.Console"
        ],
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.Hosting.Lifetime": "Information",
                "Microsoft.EntityFrameworkCore": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            }
        ],
        "Enrich": [
            "FromLogContext",
            "WithMachineName",
            "WithThreadId"
        ]
    },
    "PenguinCS": {
        "RandomKey": "houdini",
        "LegacyVersion": 153,
        "VanillaVersion": 253,
        "PreActivationDays": 7,
        "AuthTTLSeconds": 600
    }
}